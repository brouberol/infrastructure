---
title: "La Tannerie Inc."
subtitle: "Des services qu'ils sont bien"
icon: "fas fa-hotel"

header: true # Set to false to hide the header
footer: '<p>Created with <a href="https://github.com/bastienwirtz/homer">Homer</a>. Make changes on <a href="https://github.com/brouberol/infrastructure/blob/master/playbooks/roles/pi/homer/templates/config.yml.j2"><i class="fab fa-github-alt"></i></a>.</p>'

columns: "auto" # "auto" or number (must be a factor of 12: 1, 2, 3, 4, 6, 12)
connectivityCheck: true # whether you want to display a message when the apps are not accessible anymore (VPN disconnected for example)

# Optional: Proxy / hosting option
proxy:
  useCredentials: false # send cookies & authorization headers when fetching service specific data. Set to `true` if you use an authentication proxy. Can be overrided on service level.

# Optional theming
theme: default # 'default' or one of the themes available in 'src/assets/themes'.

# Optional message
message: {}

# Optional navbar
links: []

stylesheet:
- "assets/custom.css"

colors:
  dark:
    highlight-primary: "#181C3A"
    highlight-secondary: "#181C3A"
    highlight-hover: "#1F2347"
    background: "#12152B"
    card-background: "#181C3A"
    text: "#eaeaea"
    text-header: "#7C71DD"
    text-title: "#fafafa"
    text-subtitle: "#8B8D9C"
    card-shadow: rgba(0, 0, 0, 0.5)
    link: "#3273dc"
    link-hover: "#ffdd57"
    background-image: "../assets/wallpaper.jpeg"
  light:
    highlight-primary: "#fff5f2"
    highlight-secondary: "#bb41a4"
    highlight-hover: "#bebebe"
    background: "#12152B"
    card-background: "rgba(255, 245, 242, 0.8)"
    text: "#ffffff"
    text-header: "#651abd"
    text-title: "#000000"
    text-subtitle: "#111111"
    card-shadow: rgba(0, 0, 0, 0.5)
    link: "#3273dc"
    link-hover: "#2e4053"
    background-image: "../assets/wallpaper-light.jpeg"

# Services
# First level array represents a group.
# Leave only a "items" key if not using group (group name, icon & tagstyle are optional, section separation will not be displayed).
services:
  - name: "In-house media"
    icon: "fas fa-couch"
    items:
      - name: "Transmission"
        icon: "fas fa-download"
        subtitle: "Download torrents"
        url: "http://{{ peers['pi']['internal_ip'] }}:{{ transmission_rpc_port }}"
        tag: "pi"
        target: "_blank"
      - name: "Sonarr"
        icon: "fas fa-broadcast-tower"
        subtitle: "Automatic series downloader"
        url: "http://{{ peers['gallifrey']['wireguard']['ip'] }}:{{ sonarr_port }}"
        tag: "pistou"
        type: Sonarr
        apikey: "{{ sonarr_api_key }}"
        target: "_blank"
      - name: "Radarr"
        icon: "fas fa-video"
        subtitle: "Automatic movie downloader"
        url: "http://{{ peers['gallifrey']['wireguard']['ip'] }}:{{ radarr_port }}"
        tag: "pistou"
        type: Radarr
        apikey: "{{ radarr_api_key }}"
        target: "_blank"
      - name: "Jackett"
        icon: "fas fa-hat-cowboy"
        subtitle: "Torrent tracker manager"
        url: "http://{{ peers['gallifrey']['wireguard']['ip'] }}:{{ jackett_port }}"
        tag: "pistou"
        target: "_blank"
      - name: "Bazarr"
        icon: "fas fa-closed-captioning"
        subtitle: "Automatic subtitle downloader"
        url: "http://{{ peers['gallifrey']['wireguard']['ip'] }}:{{ bazarr_port }}"
        tag: "pistou"
        target: "_blank"
      - name: "Synology"
        subtitle: "Web admin of the Synology NAS"
        icon: "fas fa-server"
        url: "http://{{ peers['chambonas']['internal_ip'] }}:{{ syno_http_port }}/"
        tag: "nas"
        target: "_blank"
      - name: "Photos"
        subtitle: "Photo explorer"
        icon: "fas fa-images"
        url: "http://{{ peers['chambonas']['internal_ip'] }}:{{ syno_http_port }}/?launchApp=SYNO.Foto.AppInstance"
        target: "_blank"
        tag: nas
  - name: "In-house network"
    icon: "fas fa-house-user"
    items:
      - name: "Pi-hole"
        target: "_blank"
        type: "PiHole"
        icon: "fas fa-stop-circle"
        url: "http://{{ peers['pi']['internal_ip'] }}:{{ pihole_admin_port }}/admin"
        tag: pi
  - name: "External"
    icon: "fas fa-cloud"
    items:
      - name: "Webpage"
        icon: "fas fa-file"
        subtitle: "Personal page"
        url: "https://{{ main_domain }}"
        tag: gallifrey
      - name: "Blog"
        icon: "fas fa-blog"
        subtitle: "Personal blog"
        url: "https://{{ blog_domain }}"
        tag: gallifrey
      - name: "Isso"
        icon: "fas fa-comments"
        subtitle: "Comments manager"
        url: "https://{{ isso_domain }}/admin"
        tag: gallifrey
      - name: "Fathom"
        icon: "fas fa-chart-bar"
        subtitle: "Stats on blog visits"
        url: "https://{{ fathom_domain }}"
        tag: gallifrey
      - name: "Radicale"
        icon: "fas fa-id-card-alt"
        subtitle: "CardDAV manager"
        url: "https://{{ radicale_domain }}"
        tag: gallifrey
      - name: "Calibre"
        icon: "fas fa-book"
        subtitle: "E-Book manager"
        url: "https://{{ calibre_domain }}"
        tag: gallifrey
      - name: "Paperless"
        icon: "fas fa-file-invoice"
        url: "https://{{ paperless_domain }}"
        tag: gallifrey
        type: PaperlessNG
        apikey: "{{ paperless_api_key }}"
      - name: "Yarr "
        subtitle: "RSS aggregator"
        icon: "fas fa-rss"
        url: "https://{{ yarr_domain }}"
        tag: gallifrey
      - name: "Silverbullet"
        subtitle: "Every day notes"
        icon: "fas fa-pencil"
        url: "https://{{ silverbullet_domain }}"
        tag: gallifrey
